{
  "name": "the-libs",
  "version": "1.0.85",
  "description": "Good unbranded code by Michael Nusair https://www.linkedin.com/in/michael-nx/",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules dist tmp package-lock.json ./.nx",
    "update": "npx npm-check-updates -u",
    "sync-versions": "node scripts/sync-versions.js",
    "build": "for d in dist/libs/*/ ; do nx build $(basename $d) ; done",
    "postbuild": "for d in dist/libs/*/ ; do mv \"$d/libs/$(basename $d)/*\" \"$d\" && rm -rf \"$d/libs\" ; done",
    "publish": "for d in dist/libs/*/ ; do (cd \"$d\" && npm publish) ; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikntech/the-libs.git"
  },
