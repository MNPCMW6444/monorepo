{
  "name": "the-libs",
  "version": "1.3.12",
  "description": "Good unbranded code by Michael Nusair https://www.linkedin.com/in/michael-nx/",
  "scripts": {
    "clean": "rm -rf node_modules dist tmp package-lock.json ./.nx",
    "update": "npx npm-check-updates -u",
    "sync-versions": "node sync-version-script.js",
    "build": "for d in libs/*/ ; do nx build $(basename $d) ; done",
    "publish-all": "for d in dist/libs/*/ ; do (cd \"$d\" && npm publish --access public) ; done",
    "build-and-publish": "npm i && npm run sync-versions && npm run build && npm run publish-all"
  },
